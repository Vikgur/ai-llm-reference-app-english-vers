# Runtime environment
APP_ENV=dev
APP_NAME=ai-llm-reference-app
APP_PORT=8080

# LLM runtime controls
LLM_DETERMINISTIC=true
LLM_MAX_TOKENS=256
LLM_MAX_CONTEXT=1024
LLM_TEMPERATURE=0.7
LLM_TOP_K=40
LLM_TOP_P=0.9

# Resource limits
LLM_MAX_REQUESTS_PER_MINUTE=60
LLM_MAX_CONCURRENCY=4

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Observability
METRICS_ENABLED=true
TRACING_ENABLED=false
